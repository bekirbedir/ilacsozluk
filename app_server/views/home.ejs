<%for(var i = 0 ; i<ilaclar.length ; i++) {%>
    <div class="col-md-3">

        <div class="panel panel-default">
            <div class="panel-heading"><b><i class="fa fa-heartbeat" aria-hidden="true"></i> <%=ilaclar[i].ad%></b></div>
            <div class="panel-body">

                <p><b><i class="fa fa-building-o" aria-hidden="true"></i> Firma: </b><span> <%=ilaclar[i].firma%></span></p>
                <p><b><i class="fa fa-check-circle" aria-hidden="true"></i> Kullanım Şekli: </b><span><%=ilaclar[i].kullanim%> </span></p>
                <p><b><i class="fa fa-times-circle" aria-hidden="true"></i> Yan Etkiler: </b><span><%=ilaclar[i].yanEtkiler%> </span></p>
                <p><b><i class="fa fa-money" aria-hidden="true"></i> Satış Fiyatı: </b><span> 8,99 TL </span></p>
                <img class="img100 thumbnail" src="<%=ilaclar[i].resimUrl%>"></img>
                <div class="panel-footer">
                    <a name="" id="" class="btn btn-success w50" href="#" role="button"><i class="fa fa-commenting-o" aria-hidden="true"></i> Yorum Yap</a>
                    <a name="" id="" class="btn btn-info w50" href="/ilac/ayrinti/<%=ilaclar[i].ad%>" role="button"><i class="fa fa-info" aria-hidden="true"></i>  Ayrıntılar</a>


                </div>


            </div>
        </div>
    </div>
    <%}%>