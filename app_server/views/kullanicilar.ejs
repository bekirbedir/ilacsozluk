<div class="container">
    <div class="row">
        <h1>Kayıtlı Kullanıcılar</h1>

        <table class="table table-hover table-responsive table-striped">
            <thead>
                <tr>
                    <th>#</th>
                    <th>AD</th>
                    <th>SOYAD</th>
                    <th>MAİL</th>
                    <th>KULLANICI ADI</th>
                    <th>İşlem</th>

                </tr>
            </thead>
            <tbody>
                <%for(var i=0; i<kullanicilar.length;i++){%>
                    <tr>




                        <td>
                            <%=i%>
                        </td>
                        <td>
                            <%=kullanicilar[i].ad%>
                        </td>
                        <td>
                            <%=kullanicilar[i].soyad%>
                        </td>
                        <td>
                            <%=kullanicilar[i].email%>
                        </td>
                        <td>
                            <%=kullanicilar[i].kullaniciAdi%>
                        </td>
                        <td>
                            <a href="/login/kullaniciSil/<%=kullanicilar[i].kullaniciAdi%>">Sil</a>
                        </td>

                    </tr>
                    <%}%>
            </tbody>

        </table>
    </div>
</div>